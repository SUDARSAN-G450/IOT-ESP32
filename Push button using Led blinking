# IOT-ESP32
IOT Project using Wowki WEBSIDE
void setup() {
  Serial.begin(115200);
  pinMode(19,OUTPUT);
  pinMode(5,OUTPUT);
  pinMode(34,INPUT);
}

void loop() {
  if (digitalRead(34)==HIGH){
    digitalWrite(19, HIGH);
    digitalWrite(5, LOW);
  }
  else{
    digitalWrite(19,LOW);
    digitalWrite(5, HIGH);
  }
}
